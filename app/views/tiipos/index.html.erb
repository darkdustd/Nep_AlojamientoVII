  <div class= "container">
<p id="notice"><%= notice %></p>

<h1>Tiipos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Codigo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tiipos.each do |tiipo| %>
      <tr>
        <td><%= tiipo.nombre %></td>
        <td><%= tiipo.codigo %></td>
        <td><%= link_to 'Show', tiipo %></td>
        <td><%= link_to 'Edit', edit_tiipo_path(tiipo) %></td>
        <td><%= link_to 'Destroy', tiipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tiipo', new_tiipo_path %>
